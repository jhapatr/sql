\par }{\rtlch\fcs1 \af2 \ltrch\fcs0 \f2\insrsid3952191 \hich\af2\dbch\af31505\loch\f2 QUESTION #1
\par \hich\af2\dbch\af31505\loch\f2 --Display the DEPARTMENT_ID, DEPARTMENT Name 
\par \hich\af2\dbch\af31505\loch\f2 --and highest salary in each department form Employee Table? 
\par 
\par \hich\af2\dbch\af31505\loch\f2 SELECT E.DEPARTMENT_ID, D.DEPARTMENT_NAME, J.MAX_SALARY FROM EMPLOYEES E
\par \hich\af2\dbch\af31505\loch\f2 JOIN\hich\af2\dbch\af31505\loch\f2  DEPARTMENTS D ON E.DEPARTMENT_ID = D.DEPARTMENT_ID
\par \hich\af2\dbch\af31505\loch\f2 JOIN JOBS J ON E.JOB_ID = J.JOB_ID;
\par 
\par 
\par \hich\af2\dbch\af31505\loch\f2 -- QUESTION # 2
\par \hich\af2\dbch\af31505\loch\f2 SELECT * FROM HR.EMPLOYEES 
\par \hich\af2\dbch\af31505\loch\f2 WHERE 
\par \hich\af2\dbch\af31505\loch\f2 TO_CHAR(HIRE_DATE, 'MON-YYYY') >='JAN-2005' AND TO_CHAR(HIRE_DATE, 'MON-YYYY') <='FEB-2007'
\par \hich\af2\dbch\af31505\loch\f2 AND
\par \hich\af2\dbch\af31505\loch\f2 DEPARTMENT_ID NOT I\hich\af2\dbch\af31505\loch\f2 N (50,60)
\par \hich\af2\dbch\af31505\loch\f2 AND
\par \hich\af2\dbch\af31505\loch\f2 SALARY > 4500;
\par 
\par 
\par \hich\af2\dbch\af31505\loch\f2 -- QUESTION#3 Display unique City and department Name? *
\par \hich\af2\dbch\af31505\loch\f2 SELECT DISTINCT  CITY AS "NAMES" FROM LOCATIONS
\par \hich\af2\dbch\af31505\loch\f2 UNION
\par \hich\af2\dbch\af31505\loch\f2 SELECT DISTINCT DEPARTMENT_NAME "NAMES" FROM DEPARTMENTS;
\par 
\par \hich\af2\dbch\af31505\loch\f2 SELECT DISTINCT  CITY AS "NAMES" FROM LOCATIONS;
\par \hich\af2\dbch\af31505\loch\f2 SELECT DISTINCT DEPARTMENT_NAME "NAMES" FROM DEPARTMENTS;
\par 
\par \hich\af2\dbch\af31505\loch\f2 --QUESTION #4 Country wise employees highest and avg salary? *
\par 
\par \hich\af2\dbch\af31505\loch\f2 SELECT C.COUNTRY_NAME, MAX(E.SALARY) AS MAX_SALARY_COUNTRYWISE, ROUND(AVG(E.SALARY))\hich\af2\dbch\af31505\loch\f2  AS AVG_SALARY_COUNTRYWISE FROM EMPLOYEES E 
\par \hich\af2\dbch\af31505\loch\f2 JOIN DEPARTMENTS D ON E.DEPARTMENT_ID = D.DEPARTMENT_ID
\par \hich\af2\dbch\af31505\loch\f2 JOIN LOCATIONS L ON D.LOCATION_ID = L.LOCATION_ID
\par \hich\af2\dbch\af31505\loch\f2 JOIN COUNTRIES C ON L.COUNTRY_ID = C.COUNTRY_ID
\par \hich\af2\dbch\af31505\loch\f2 GROUP BY C.COUNTRY_NAME;
\par 
\par \hich\af2\dbch\af31505\loch\f2 --QUESTION#5 Display City wise \hich\af2\dbch\af31505\loch\f2 avg salary?
\par \hich\af2\dbch\af31505\loch\f2 SELECT l.city, ROUND(AVG(E.SALARY)) AS AVG_SALARY_COUNTRYWISE FROM EMPLOYEES E 
\par \hich\af2\dbch\af31505\loch\f2 JOIN DEPARTMENTS D ON E.DEPARTMENT_ID = D.DEPARTMENT_ID
\par \hich\af2\dbch\af31505\loch\f2 JOIN LOCATIONS L ON D.LOCATION_ID = L.LOCATION_ID
\par \hich\af2\dbch\af31505\loch\f2 JOIN COUNTRIES C ON L.COUNTRY_ID = C.COUNTRY_ID
\par \hich\af2\dbch\af31505\loch\f2 GROUP BY L\hich\af2\dbch\af31505\loch\f2 .CITY;
