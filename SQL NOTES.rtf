SQL developer preview youtube video practice SQL ONLINE r.po_no AS "PO NUMBER", r.rpo_purchase_do AS "PURCHASE ORDER LIMIT", r.change_dt, r.notes, SUM(r.upo_purchase_do) AS "PURCHASE ORDER", SUM(r.purchase_cco_do) AS "CHANGE ORDER", SUM(r.upo_purchase_do) + SUM(r.purchase_cco_do) AS "TOTAL", SUM(r.ordered) AS "UNITS ON ORDER", SUM(r.active) AS "ACTIVE UNITS", r."CREATION DATE",\par
CASE\par
WHEN SUM(r.ordered) > 0\par
  AND\par
  SUM(r.active) IS NULL THEN\par
  'ORDERED'\par
WHEN SUM(r.ordered) IS NULL\par
  AND\par
  SUM(r.active) > 0 THEN\par
  'ACTIVE'\par
WHEN SUM(r.ordered) > 0\par
  AND\par
  SUM(r.active) > 0 THEN\par
  'MIXED'\par
END AS status FROM (\par
          SELECT    rpo.change_dt,\par
                    rpo.notes,\par
                    rpo.po_no,\par
                    rpo.purchase_do AS rpo_purchase_do,\par
                    upo.purchase_do AS upo_purchase_do,\par
                    upo.purchase_cco_do,\par
                    CASE\par
                              WHEN um.status IN ( 'O',\par
                                                 'B' ) THEN 1\par
                    END AS "ORDERED",\par
                    CASE\par
                              WHEN um.status IN ( 'A',\par
                                                 'D' ) THEN 1\par
                    END AS "ACTIVE",\par
                    um.acquis_dt,\par
                    to_date(to_char(rpo.purchase_dt,'MM/DD/YYYY'),'MM/DD/YYYY') AS "CREATION DATE",\par
                    to_date(to_char(rpo.change_dt,'MM/DD/YYYY'),'MM/DD/YYYY')   AS "PO CHANGE DATE"\par
          FROM      mfive.unitrequestpo rpo\par
          left join mfive.unit_po upo\par
          ON        upo.company = rpo.company\par
          AND       upo.po_no = rpo.po_no\par
          left join mfive.unit_dept_comp_main um\par
          ON        um.company = rpo.company\par
          AND       um.unit_id = upo.unit_id\par
          WHERE     rpo.company = 'SDGE'\par
          AND       rpo.po_status_cd IN ( 'O',\par
                                         'B' )\par
          AND       ( (\par
                                        trim(rpo.notes) LIKE ( 'FIXED_%' )\par
                              AND       to_date(substr(trim(rpo.notes),7,10),'MM/DD/YYYY') < to_date(to_char(rpo.change_dt,'MM/DD/YYYY'),'MM/DD/YYYY') )\par
                    OR        (\par
                                        trim(rpo.notes) NOT LIKE ( 'FIXED_%' )\par
                              OR        rpo.notes IS NULL ) )\par
          AND       ( (\par
                                        rpo.purchase_do = 0\par
                              AND       rpo.purchase_dt >= '01-JAN-2016' )\par
                    OR        rpo.purchase_do > 0 )\par
          AND       um.unit_type = 'U'\par
          AND       um.unit_id NOT IN\par
                    (\par
                           SELECT ud.unit_id\par
                           FROM   mfive.unitdis ud\par
                           WHERE  ud.dis_status_cd IN ( 'R' )\par
                           AND    ud.company = 'SDGE' )\par
                    --AND UM.ASSET_TYPE = 'UNIT'\par
          AND       um.status NOT IN ( 'S' )\par
                    --(UM.STATUS IN ('O','B') OR (RPO.CHANGE_DT > UM.ACQUIS_DT AND UM.STATUS NOT IN ('S') ))\par
          ORDER BY  1 ) r GROUP BY r.po_no, r.rpo_purchase_do, r."CREATION DATE", r.notes, r.change_dt \par
