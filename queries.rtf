SELECT\par
    r.po_no AS "PO NUMBER",\par
    r.rpo_purchase_do AS "PURCHASE ORDER LIMIT",\par
    r.change_dt,\par
    r.notes,\par
    SUM(r.upo_purchase_do) AS "PURCHASE ORDER",\par
    SUM(r.purchase_cco_do) AS "CHANGE ORDER",\par
    SUM(r.upo_purchase_do) + SUM(r.purchase_cco_do) AS "TOTAL",\par
    SUM(r.ordered) AS "UNITS ON ORDER",\par
    SUM(r.active) AS "ACTIVE UNITS",\par
    r."CREATION DATE",\par
    CASE\par
            WHEN SUM(r.ordered) > 0\par
                 AND SUM(r.active) IS NULL THEN 'ORDERED'\par
            WHEN SUM(r.ordered) IS NULL\par
                 AND SUM(r.active) > 0 THEN 'ACTIVE'\par
            WHEN SUM(r.ordered) > 0\par
                 AND SUM(r.active) > 0 THEN 'MIXED'\par
        END\par
    AS status\par
FROM\par
    (\par
        SELECT\par
            rpo.change_dt,\par
            rpo.notes,\par
            rpo.po_no,\par
            rpo.purchase_do AS rpo_purchase_do,\par
            upo.purchase_do AS upo_purchase_do,\par
            upo.purchase_cco_do,\par
            CASE\par
                    WHEN um.status IN (\par
                        'O',\par
                        'B'\par
                    ) THEN 1\par
                END\par
            AS "ORDERED",\par
            CASE\par
                    WHEN um.status IN (\par
                        'A',\par
                        'D'\par
                    ) THEN 1\par
                END\par
            AS "ACTIVE",\par
            um.acquis_dt,\par
            TO_DATE(TO_CHAR(rpo.purchase_dt,'MM/DD/YYYY'),'MM/DD/YYYY') AS "CREATION DATE",\par
            TO_DATE(TO_CHAR(rpo.change_dt,'MM/DD/YYYY'),'MM/DD/YYYY') AS "PO CHANGE DATE"\par
        FROM\par
            mfive.unitrequestpo rpo\par
            LEFT JOIN mfive.unit_po upo ON upo.company = rpo.company\par
                                           AND upo.po_no = rpo.po_no\par
            LEFT JOIN mfive.unit_dept_comp_main um ON um.company = rpo.company\par
                                                      AND um.unit_id = upo.unit_id\par
        WHERE\par
            rpo.company = 'SDGE'\par
            AND rpo.po_status_cd IN (\par
                'O',\par
                'B'\par
            )\par
            AND (\par
                (\par
                    TRIM(rpo.notes) LIKE ( 'FIXED_%' )\par
                    AND TO_DATE(substr(TRIM(rpo.notes),7,10),'MM/DD/YYYY') < TO_DATE(TO_CHAR(rpo.change_dt,'MM/DD/YYYY'),'MM/DD/YYYY')\par
                )\par
                OR (\par
                    TRIM(rpo.notes) NOT LIKE ( 'FIXED_%' )\par
                    OR rpo.notes IS NULL\par
                )\par
            )\par
            AND (\par
                (\par
                    rpo.purchase_do = 0\par
                    AND rpo.purchase_dt >= '01-JAN-2016'\par
                )\par
                OR rpo.purchase_do > 0\par
            )\par
            AND um.unit_type = 'U'\par
            AND um.unit_id NOT IN (\par
                SELECT\par
                    ud.unit_id\par
                FROM\par
                    mfive.unitdis ud\par
                WHERE\par
                    ud.dis_status_cd IN (\par
                        'R'\par
                    )\par
                    AND ud.company = 'SDGE'\par
            )\par
--AND UM.ASSET_TYPE = 'UNIT'\par
            AND um.status NOT IN (\par
                'S'\par
            )\par
--(UM.STATUS IN ('O','B') OR (RPO.CHANGE_DT > UM.ACQUIS_DT AND UM.STATUS NOT IN ('S') ))\par
        ORDER BY\par
            1\par
    ) r\par
GROUP BY\par
    r.po_no,\par
    r.rpo_purchase_do,\par
    r."CREATION DATE",\par
    r.notes,\par
    r.change_dt\par
